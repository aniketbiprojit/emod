{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@db/database/(.*)": "<rootDir>/../../../libs/database/src/$1",
    "@db/database": "<rootDir>/../../../libs/database/src",
    "@storage/local-storage/(.*)": "<rootDir>/../../../libs/local-storage/src/$1",
    "@storage/local-storage": "<rootDir>/../../../libs/local-storage/src"
  }
}
